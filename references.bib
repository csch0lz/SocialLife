@article{pachuckiMentalHealthSocial2015,
  title = {Mental Health and Social Networks in Early Adolescence: {{A}} Dynamic Study of Objectively-Measured Social Interaction Behaviors},
  shorttitle = {Mental Health and Social Networks in Early Adolescence},
  author = {Pachucki, Mark C. and Ozer, Emily J. and Barrat, Alain and Cattuto, Ciro},
  year = {2015},
  month = jan,
  journal = {Social Science \& Medicine},
  series = {Special {{Issue}}: {{Social Networks}}, {{Health}} and {{Mental Health}}},
  volume = {125},
