@article{cosmeMessageSelfSocial2022,
  title = {Message Self and Social Relevance Increases Intentions to Share Content: {{Correlational}} and Causal Evidence from Six Studies},
  shorttitle = {Message Self and Social Relevance Increases Intentions to Share Content},
  author = {Cosme, Danielle and Scholz, Christin and Chan, Hang-Yee and Dor{\'e}, Bruce P. and Pandey, Prateekshit and {Carreras-Tartak}, Jos{\'e} and Cooper, Nicole and Paul, Alexandra and Burns, Shannon M. and Falk, Emily B.},
