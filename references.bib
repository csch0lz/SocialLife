@article{badell-grauInvestigatingPrevalenceReactive2020,
  title = {Investigating the {{Prevalence}} of {{Reactive Online Searching}} in the {{COVID-19 Pandemic}}: {{Infoveillance Study}}},
  shorttitle = {Investigating the {{Prevalence}} of {{Reactive Online Searching}} in the {{COVID-19 Pandemic}}},
  author = {{Badell-Grau}, Rafael A. and Cuff, Jordan Patrick and Kelly, Brendan P. and {Waller-Evans}, Helen and {Lloyd-Evans}, Emyr},
