@article{bediIncubationCueInducedCigarette2011,
  title = {Incubation of {{Cue-Induced Cigarette Craving During Abstinence}} in {{Human Smokers}}},
  author = {Bedi, Gillinder and Preston, Kenzie L. and Epstein, David H. and Heishman, Stephen J. and Marrone, Gina F. and Shaham, Yavin and {de Wit}, Harriet},
