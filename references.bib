@article{ahernNeighborhoodSmokingNorms2009,
  title = {Neighborhood Smoking Norms Modify the Relation between Collective Efficacy and Smoking Behavior},
  author = {Ahern, Jennifer and Galea, Sandro and Hubbard, Alan and Syme, S. Leonard},
  year = {2009},
