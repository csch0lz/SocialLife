@article{bethlehemOxytocinBrainPhysiology2013,
  title = {Oxytocin, Brain Physiology, and Functional Connectivity: {{A}} Review of Intranasal Oxytocin {{fMRI}} Studies},
  shorttitle = {Oxytocin, Brain Physiology, and Functional Connectivity},
  author = {Bethlehem, Richard A. I. and {van Honk}, Jack and Auyeung, Bonnie and {Baron-Cohen}, Simon},
  year = {2013},
  month = jul,
  journal = {Psychoneuroendocrinology},
  volume = {38},
  number = {7},
