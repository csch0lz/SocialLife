@article{morenoContentAnalysisDisplayed2010,
  title = {A {{Content Analysis}} of {{Displayed Alcohol References}} on a {{Social Networking Web Site}}},
  author = {Moreno, Megan A. and Briner, Leslie R. and Williams, Amanda and Brockman, Libby and Walker, Leslie and Christakis, Dimitri A.},
