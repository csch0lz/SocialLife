@article{slavikExaminingTweetContent2021,
  title = {Examining Tweet Content and Engagement of {{Canadian}} Public Health Agencies and Decision Makers during {{COVID-19}}: {{Mixed}} Methods Analysis},
  shorttitle = {Examining {{Tweet Content}} and {{Engagement}} of {{Canadian Public Health Agencies}} and {{Decision Makers During COVID-19}}},
  author = {Slavik, Catherine E. and Buttle, Charlotte and Sturrock, Shelby L. and Darlington, J. Connor and Yiannakoulias, Niko},
