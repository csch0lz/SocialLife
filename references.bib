  keywords = {*Cues,*Environment,*Extinction (Learning),*Smoking Cessation,*Tobacco Smoking,Associative Processes},
  file = {/Users/cscholz/Zotero/storage/HQAGE8PJ/Conklin - 2006 - Environments as cues to smoke Implications for hu.pdf}
}

@article{conklinPeopleServeCues2013,
  title = {Do {{People Serve}} as {{Cues}} to {{Smoke}}?},
  author = {Conklin, Cynthia A. and Salkeld, Ronald P. and Perkins, Kenneth A. and Robin, Nathalie},
  year = {2013},
  month = jul,
  journal = {Nicotine \& Tobacco Research},
