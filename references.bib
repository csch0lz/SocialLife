@inproceedings{pangMonitoringAdolescentAlcohol2015,
  title = {Monitoring Adolescent Alcohol Use via Multimodal Analysis in Social Multimedia},
  booktitle = {2015 {{IEEE International Conference}} on {{Big Data}} ({{Big Data}})},
  author = {Pang, Ran and Baretto, Agustin and Kautz, Henry and Luo, Jiebo},
  year = {2015},
