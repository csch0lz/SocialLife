  title = {Avoiding Sedentary Behaviors Requires More Cortical Resources than Avoiding Physical Activity: {{An EEG}} Study},
  shorttitle = {Avoiding Sedentary Behaviors Requires More Cortical Resources than Avoiding Physical Activity},
  author = {Cheval, Boris and Tipura, Eda and Burra, Nicolas and Frossard, Jaromil and Chanal, Julien and Orsholits, Dan and Radel, R{\'e}mi and Boisgontier, Matthieu P.},
