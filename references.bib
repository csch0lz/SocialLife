  doi = {10.1037/a0029599},
  abstract = {The association between smokers' cue-induced craving and subsequent ability to initiate abstinence is unclear. Dependent smokers (N = 158) completed a single cue-reactivity session prior to participating in a larger within-subjects study, which independently examined predictors of initiating quitting during 5 days each on nicotine versus placebo patch. In the larger study, all smokers used nicotine and placebo patch (double blind) for 1 week each following a preceding week of ad lib smoking, in a 2 {\texttimes} 2 cross-over design. Generalized estimating equation (GEE) models determined the predictive ability of cue-induced craving (cue reactivity) on subsequent success at initiating a quit attempt (at least 24 hr quit) for each patch condition. Smokers who exhibited greater craving during exposure to smoking cues had significantly greater odds of successfully initiating abstinence during either quit attempt week (i.e., the nicotine or placebo patch week). This relationship was not statistically significant for self-reported craving in response to neutral cues. However, a greater smoking-neutral cue difference score for cue-induced craving was also a significant predictor of successfully initiating abstinence, but only among those not monetarily reinforced. Implications of these seemingly counterintuitive findings are discussed.},
  copyright = {(c) 2012 APA, all rights reserved},
  keywords = {*Craving,*Cues,*Drug Abstinence,*Relapse (Disorders),Smoking cessation},
  file = {/Users/cscholz/Zotero/storage/B42TVNSE/Conklin et al. - 2012 - Cue reactivity as a predictor of successful abstin.pdf}
}

