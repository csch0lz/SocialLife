@article{kesterenRetrievalAssociativeInformation2010,
  title = {Retrieval of {{Associative Information Congruent}} with {{Prior Knowledge Is Related}} to {{Increased Medial Prefrontal Activity}} and {{Connectivity}}},
  author = {van Kesteren, Marlieke T. R. and Rijpkema, Mark and Ruiter, Dirk J. and Fern{\'a}ndez, Guill{\'e}n},
  year = {2010},
