@article{masseyApplyingMultipleData2016,
  title = {Applying {{Multiple Data Collection Tools}} to {{Quantify Human Papillomavirus Vaccine Communication}} on {{Twitter}}},
  author = {Massey, Philip M. and Leader, Amy and {Yom-Tov}, Elad and Budenz, Alexandra and Fisher, Kara and Klassen, Ann C.},
