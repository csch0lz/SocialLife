@article{jerniganAlcoholMarketingYouth2017,
  title = {Alcohol Marketing and Youth Alcohol Consumption: A Systematic Review of Longitudinal Studies Published since 2008},
  shorttitle = {Alcohol Marketing and Youth Alcohol Consumption},
  author = {Jernigan, David and Noel, Jonathan and Landon, Jane and Thornton, Nicole and Lobstein, Tim},
