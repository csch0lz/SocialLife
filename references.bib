@article{wiersEffectsCognitiveBias2014,
  title = {Effects of {{Cognitive Bias Modification Training}} on {{Neural Alcohol Cue Reactivity}} in {{Alcohol Dependence}}},
  author = {Wiers, Corinde E. and Stelzel, Christine and Gladwin, Thomas E. and Park, Soyoung Q. and Pawelczack, Steffen and Gawron, Christiane K. and Stuke, Heiner and Heinz, Andreas and Wiers, Reinout W. and Rinck, Mike and Lindenmeyer, Johannes and Walter, Henrik and Bermpohl, Felix},
  year = {2014},
