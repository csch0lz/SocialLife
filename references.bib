  author = {Venkatraman, Vinod and Dimoka, Angelika and Pavlou, Paul A. and Vo, Khoi and Hampton, William and Bollinger, Bryan and Hershfield, Hal E. and Ishihara, Masakazu and Winer, Russell S.},
