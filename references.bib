  shorttitle = {Smoking, Smoking Cessation and Smoking Relapse Patterns},
  author = {Yeomans, K. and Payne, K. A. and Marton, J. P. and Merikle, E. P. and Proskorovsky, I. and Zou, K. H. and Li, Q. and Willke, R. J.},
  year = {2011},
  month = oct,
  journal = {International Journal of Clinical Practice},
  volume = {65},
