@article{limDecisionValueComputations2011,
  title = {The {{Decision Value Computations}} in the {{vmPFC}} and {{Striatum Use}} a {{Relative Value Code That}} Is {{Guided}} by {{Visual Attention}}},
  author = {Lim, Seung-Lark and O'Doherty, John P. and Rangel, Antonio},
