@article{huangPeerInfluencesImpact2014,
  title = {Peer {{Influences}}: {{The Impact}} of {{Online}} and {{Offline Friendship Networks}} on {{Adolescent Smoking}} and {{Alcohol Use}}},
  shorttitle = {Peer {{Influences}}},
  author = {Huang, Grace C. and Unger, Jennifer B. and Soto, Daniel and Fujimoto, Kayo and Pentz, Mary Ann and {Jordan-Marsh}, Maryalice and Valente, Thomas W.},
  year = {2014},
  month = may,
  journal = {Journal of Adolescent Health},
