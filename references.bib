@article{genevskyWhenBrainBeats2017,
  title = {When {{Brain Beats Behavior}}: {{Neuroforecasting Crowdfunding Outcomes}}},
  shorttitle = {When {{Brain Beats Behavior}}},
  author = {Genevsky, Alexander and Yoon, Carolyn and Knutson, Brian},
  year = {2017},
  month = sep,
  journal = {The Journal of Neuroscience: The Official Journal of the Society for Neuroscience},
  volume = {37},
