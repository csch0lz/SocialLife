@article{erevikWhoWatchingUsergenerated2018,
  title = {Who Is Watching User-Generated Alcohol Posts on Social Media?},
  author = {Erevik, Eilin K. and Pallesen, St{\aa}le and Andreassen, Cecilie S. and Vedaa, {\O}ystein and Torsheim, Torbj{\o}rn},
