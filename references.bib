@article{altenburgEffectsOneTwo2016,
  title = {Effects of One versus Two Bouts of Moderate Intensity Physical Activity on Selective Attention during a School Morning in {{Dutch}} Primary Schoolchildren: {{A}} Randomized Controlled Trial},
  shorttitle = {Effects of One versus Two Bouts of Moderate Intensity Physical Activity on Selective Attention during a School Morning in {{Dutch}} Primary Schoolchildren},
  author = {Altenburg, Teatske M. and Chinapaw, Mai J. M. and Singh, Amika S.},
  year = {2016},
