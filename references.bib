@incollection{kramerOnlineSelfPresentationBalancing2011,
  title = {Online {{Self-Presentation}}: {{Balancing Privacy Concerns}} and {{Impression Construction}} on {{Social Networking Sites}}},
  shorttitle = {Online {{Self-Presentation}}},
  booktitle = {Privacy {{Online}}},
