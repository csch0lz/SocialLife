  shorttitle = {Increasing the Dose of Television Advertising in a National Antismoking Media Campaign},
  author = {McAfee, Tim and Davis, Kevin C. and Shafer, Paul and Patel, Deesha and Alexander, Robert and Bunnell, Rebecca},
