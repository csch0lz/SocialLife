  keywords = {{Feedback, Psychological},Adolescent,Adult,Aggression,Anger,Brain Mapping,Cerebral Cortex,emotion regulation,Female,functional magnetic resonance imaging (fMRI),Humans,Interpersonal Relations,Magnetic Resonance Imaging,Male,Peer Group,Self-Control,social acceptance,social evaluation,social rejection,Young Adult},
