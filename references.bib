@article{sternUpdatingBeliefsDecision2010a,
  ids = {sternUpdatingBeliefsDecision2010},
  title = {Updating {{Beliefs}} for a {{Decision}}: {{Neural Correlates}} of {{Uncertainty}} and {{Underconfidence}}},
  shorttitle = {Updating {{Beliefs}} for a {{Decision}}},
  author = {Stern, Emily R. and Gonzalez, Richard and Welsh, Robert C. and Taylor, Stephan F.},
