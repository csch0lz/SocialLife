@article{quickExaminingUseForceful2008,
  title = {Examining the {{Use}} of {{Forceful Language When Designing Exercise Persuasive Messages}} for {{Adults}}: {{A Test}} of {{Conceptualizing Reactance Arousal}} as a {{Two-Step Process}}},
